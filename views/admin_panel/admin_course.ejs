<%- include ('../sections/admin_header') %>

  <%- include ('../sections/admin_create') %>


  <section id="main">
    <div class="container">
      <div class="row">
       
        <%- include ('../sections/admin_dashboard') %>


        <div class="col-md-9">
          <!-- Website Overview -->
          <div class="panel panel-default">
            <div class="panel-heading main-color-bg">
              <h3 class="panel-title">Course</h3>
            </div>
            <div class="panel-body">
              <br>
              <table class="table table-striped table-hover">
                <tr>
                  <th>Course ID</th>
                  <th>Course Name</th>
                  <th>Faculty Name</th>
                  <th>Department</th>
                  <th></th>
                </tr>
                <% courses.forEach(function(courses){ %>
                <tr>
                  <td><%= courses.c_id %></td>
                  <td><%= courses.course_name %></td>
                  <td><%= courses.faculty_name %></td>
                  <td><%= courses.department %></td>
                  <td> <a class="btn btn-danger" href="#">Delete</a></td>
                </tr>
                <%  }); %>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <footer id="footer">
    <p>Copyright Student Management System, &copy; 2020</p>
  </footer>

 <!-- Modals -->

  <!-- Add faculty -->
  <%- include ('../sections/add_faculty') %>


  <!-- Add Student -->
  <%- include ('../sections/add_student') %>

  <!-- Add Courses -->
  <%- include ('../sections/add_course') %>


  <!-- Add Department -->
  <%- include ('../sections/add_department') %>

  <!-- Bootstrap core JavaScript
    ================================================== -->
  <!-- Placed at the end of the document so the pages load faster -->
  <%- include ('../sections/admin_footer') %>